{
  "version": 2,
  "public": true,
  "builds": [
  {
    "src": "src/server.js",
    "use": "@vercel/node"
  }
],
  "routes": [
   
    {
      "src": "/users",
      "dest": "/src/routes/user.routes.js"
    },
    {
      "src": "/notes",
      "dest": "/src/routes/notes.routes.js"
    },

    {
      "src": "/sessions",
      "dest": "/src/routes/sessions.routes.js"
    },

    {
      "src": "/tags",
      "dest": "/src/routes/tags.routes.js"
    }

  ]
}

